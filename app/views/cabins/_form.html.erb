
  <%= simple_form_for @cabin do |f| %>
    <%= f.input :name %>
    <%= f.input :picture %>
    <%= f.input :city %>
    <%= f.input :address %>
    <%= f.input :daily_rate %>
    <%= f.input :description %>
    <%= f.input :rating %>
    <%= f.input :capacity %>
    <%= f.input :kitchen %>
    <%= f.input :lake %>
